{
  "compilerOptions": {
    "incremental": true,
    "lib": [
      "ES2022"
    ],
    "target": "es2022",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "composite": true,
    "rootDir": "src",
    "outDir": "dist",
  },
  "exclude": [
    "dist",
  ]
}